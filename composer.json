{
    "name": "oro/mailchimp",
    "description": "Oro MailChimp integration",
    "homepage": "https://github.com/oroinc/OroMailChimpBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.oroinc.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\MailChimpBundle\\": ""},
        "exclude-from-classmap": ["**/Tests/"]
    },
    "require": {
        "drewm/mailchimp-api": "^2.5",
        "oro/crm": "5.0.*",
        "oro/marketing": "5.0.*"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "conflict": {
        "oro/crm-mail-chimp": "*"
    }
}
